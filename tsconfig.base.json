{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "strict": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}